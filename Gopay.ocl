package topUpGame


context Gopay::Gopay()
    post : self.saldo = 0


context Gopay::Gopay(ammount: Real)

context Gopay::getNamaDompet() : String
    post : result = "Gopay"

context Gopay::bayar() : Boolean
    init : false
    inv : self.saldo >= 0
    pre : self.getSaldo() >= tagihan
    post : self@pre.getSaldo() - self.getSaldo = tagihan
         and result = true


endpackage